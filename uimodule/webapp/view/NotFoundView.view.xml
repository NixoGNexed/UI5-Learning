<mvc:View 
    controllerName="com.myorg.ui5learning.controller.NotFoundView"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page 
        id="page"
        title="{i18n>appTitle}"
        showNavButton="true"
        navButtonPress="onNavBack">
        <content>
            <FlexBox direction="Column" alignItems="Center" justifyContent="Center">
                <Text class="sapUiLargeMarginTop notFoundTitle" text="{i18n>notFoundTitle}"/>
                <Image class="sapUiSmallMarginTop" src="./img/sadbee.png"></Image>
            </FlexBox>
        </content>
    </Page>
</mvc:View>
